{
  "name": "mbeurel/cv",
  "description": "",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "symfony/webpack-encore-bundle": "v2.1.1",
    "symfony/console": "6.4.*",
    "symfony/yaml": "6.4.*",
    "symfony/flex": "^2",
    "symfony/options-resolver": "^6.4",
    "symfony/dotenv": "6.4.*",
    "symfony/http-kernel": "6.4.*",
    "symfony/http-foundation": "6.4.*",
    "symfony/ux-twig-component": "v2.25.2",
    "symfony/runtime": "6.4.*",
    "symfony/framework-bundle": "6.4.*",
    "twig/twig": "^v3.1.0",
    "symfony/twig-bridge": "6.4.*",
    "symfony/twig-bundle": "6.4.*"
  },
  "require-dev": {
    "symfony/debug-bundle": "^6.4",
    "symfony/var-dumper": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "prefer-stable": true,
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*",
      "docker": false
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/runtime": true,
      "symfony/flex": true
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
